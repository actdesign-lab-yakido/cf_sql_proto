<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <title>ユーザー一覧</title>
</head>
<body>
    <p><a href="<%= cf_root_path %>/create">ユーザ追加</a></p>
    <table>
        <tr>
            <th>USER ID</th>
            <th>名前</th>
            <th>email</th>
            <th>更新</th>
            <th>削除</th>
        </tr>
        <% users.forEach(function (value) { %>
        <tr>
            <td><%= value.id %></td>
            <td><%= value.name %></td>
            <td><%= value.email %></td>
            <td><a href="<%= cf_root_path %>/edit/<%= value.id %>"  class="btn-border">更新</a></td>
            <td><a href="<%= cf_root_path %>/delete/<%= value.id %>"  class="btn-border">削除</a></td>
        </tr>
        <% }); %>
    </table>
</body>
</html>
